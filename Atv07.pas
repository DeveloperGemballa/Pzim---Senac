Program Operacoes ;
	var multiplicacao1,multiplicacao2:integer;
	divisao1,divisao2:real;
Begin
   write('A multiplicação de 12X4 é: ');
   multiplicacao1:= 12+12+12+12;
   writeln(multiplicacao1);
   
   write('A multiplicação de 23X11 é: ');
   multiplicacao2:= 23+23+23+23+23+23+23+23+23+23+23;
   writeln(multiplicacao2);
   
   write('A divisao de 10/2 é: ');
   divisao1:= 10 - 5;
   writeln(divisao1);
   
   write('A divisao de 175/7 é: ');
   divisao2:= 175 - 150;
   writeln(divisao2);
End.